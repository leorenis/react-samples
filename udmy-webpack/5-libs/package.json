  "license": "ISC",
  "description": "Training libs with vitest",
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "3.5.3",
    "vitest": "^2.1.8"
  }
}
