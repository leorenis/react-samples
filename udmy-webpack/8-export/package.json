{
  "name": "@samplesleo/validations",
  "version": "1.0.4",
  "main": "index.bundle.js",
  "type": "module",
  "private": false,
  "author": "leorenis",
  "license": "ISC",
  "keywords": [
    "cpf",
    "cnpj"
  ],
  "description": "Training libs with vitest",
  "scripts": {
    "build-dev": "webpack --config configs/webpack.dev.config.js",
    "build-production": "webpack --config configs/webpack.prod.config.js",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "v:patch": "npm version patch",
    "v:minor": "npm version minor",
    "v:major": "npm version major",
    "v:dep": "npm publish --access public",
    "v:patch:full": "npm run test && npm run build-production && npm run v:patch && npm run v:dep",
    "v:minor:full": "npm run test && npm run build-production && npm run v:minor && npm run v:dep",
    "v:major:full": "npm run test && npm run build-production && npm run v:major && npm run v:dep"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "vitest": "^2.1.8",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  }
}
